{
    "name": "wyrihaximus/phergie-dns",
    "description": "Phergie plugin for Looking up IP's by hostnames",
    "keywords": [ "irc", "bot", "react", "plugin", "phergie", "phergieplugin" ],
    "license": "MIT",
    "support": {
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/wyrihaximus/PhergieDns/issues",
        "source": "https://github.com/wyrihaximus/PhergieDns"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "react/dns": "0.4.*",
        "phergie/phergie-irc-bot-react": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phake/phake": "~1.0",
        "phergie/phergie-irc-plugin-react-command": "~1.0",
        "vectorface/dunit": "dev-master"
    },
    "suggest": {
        "phergie/phergie-irc-plugin-react-command": "Required if you want to make use of the command support"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Phergie\\Plugin\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Phergie\\Tests\\Plugin\\Dns\\": "tests"
        }
    }
}
